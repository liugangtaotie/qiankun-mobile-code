{
  "name": "qiankun-mobile-code",
  "version": "0.0.1",
  "description": "qiankun demo",
  "main": "index.js",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0"
  },
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main": "cd main && npm i",
    "install:sub-first": "cd sub-first && npm i",
    "install:sub-second": "cd sub-second && npm i",
    "start": "npm-run-all --parallel start:*",
    "start:sub-first": "cd sub-first && npm start",
    "start:sub-second": "cd sub-second && npm start",
    "start:main": "cd main && npm start",
    "stylelint": "stylelint **/**/**/**/**/*.{html,vue,css,sass,scss,less} --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "github.com:liugangtaotie/qiankun-mobile-code.git"
  },
  "keywords": [
    "qiankun",
    "qiankun-example",
    "qiankun-mobile-code",
    "vue2",
    "vite2",
    "typescript"
  ],
  "author": "wuxiang",
  "license": "MIT",
  "__npminstall_done": false
}
